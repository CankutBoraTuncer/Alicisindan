<resources>
    <style name="Theme.Alicisindan" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/primary</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>
    <style name="ImageView.Round" parent="">
        <item name="cornerSize">50%</item>
    </style>
    <style name="ImageView.AdvertisementCorner" parent="">
        <item name="cornerSizeTopRight">@dimen/_8sdp</item>
        <item name="cornerSizeTopLeft">@dimen/_8sdp</item>
        <item name="cornerSizeBottomLeft">@dimen/_8sdp</item>
        <item name="cornerSizeBottomRight">@dimen/_8sdp</item>
        <item name="cornerFamily">rounded</item>
    </style>
    <style name="Theme.Alicisindan.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.Alicisindan.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.Alicisindan.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>